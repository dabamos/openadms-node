            {
                "Init":
                [
                    {
                        "Name": "power_on",
                        "Description": "Power on sensor.",
                        "Receivers": ["PreProcessor"],
                        "NextReceiver": 0,
                        "Enabled": true,
                        "Onetime": true,
                        "AwaitTime": 0.8,
                        "SleepTime": 1.0,
                        "ID": null,
                        "Request": "A\r",
                        "ResponsePattern": "(#)",
                        "ResponseDelimiter": "\r",
                        "ResponseSets": [
                            {
                                "Description": "PowerOn",
                                "Type": "String",
                                "Unit": "none"
                            }
                        ]
                    },
                    {
                        "Name": "set_command_set",
                        "Description": "Change command set.",
                        "Receivers": ["PreProcessor"],
                        "NextReceiver": 0,
                        "Enabled": true,
                        "Onetime": true,
                        "AwaitTime": 0.8,
                        "SleepTime": 1.0,
                        "ID": null,
                        "Request": "CMDT 1\r",
                        "ResponsePattern": "(\\*)",
                        "ResponseDelimiter": "\r",
                        "ResponseSets": [
                            {
                                "Description": "SetCmd",
                                "Type": "String",
                                "Unit": "none"
                            }
                        ]
                    },
                    {
                        "Name": "save",
                        "Description": "Save changes.",
                        "Receivers": ["PreProcessor"],
                        "NextReceiver": 0,
                        "Enabled": true,
                        "Onetime": true,
                        "AwaitTime": 0.8,
                        "SleepTime": 1.0,
                        "ID": null,
                        "Request": "SAVE\r",
                        "ResponsePattern": "(\\*)",
                        "ResponseDelimiter": "\r",
                        "ResponseSets": [
                            {
                                "Description": "Save",
                                "Type": "String",
                                "Unit": "none"
                            }
                        ]
                    }
                ],
                "Main":
                [
                    {
                        "Name": "get_temperature",
                        "Description": "Get temperature.",
                        "Receivers": ["PreProcessor", "DistanceCorrector", "FileExporter"],
                        "NextReceiver": 0,
                        "Enabled": true,
                        "Onetime": false,
                        "AwaitTime": 0.8,
                        "SleepTime": 1.0,
                        "ID": "TEMP",
                        "Request": "TEMP ?\r",
                        "ResponsePattern": "([+-]?\\d+\\.+\\d)",
                        "ResponseDelimiter": "\r",
                        "ResponseSets": [
                            {
                                "Description": "Temperature",
                                "Type": "Float",
                                "Unit": "°C"
                            }
                        ]
                    },
                    {
                        "Name": "get_pressure",
                        "Description": "Get air pressure..",
